@prefix l: </>.
@prefix c: </profile/card#>.
@prefix n0: <http://xmlns.com/foaf/0.1/>.
@prefix n1: </.acl#>.

n1:owner
    a acl:Authorization;
    acl:accessTo l:;
    acl:agent c:me, <mailto:lalasepp@gmail.com>;
    acl:default l:;
    acl:mode acl:Control, acl:Read, acl:Write.
n1:public
    a acl:Authorization;
    acl:accessTo l:;
    acl:agentClass n0:Agent;
    acl:default l:;
    acl:mode acl:Read.
